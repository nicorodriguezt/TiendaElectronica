@model IEnumerable<TiendaElectronica.venta>

@{
    ViewBag.Title = "Historial";
}

<h2>Compras</h2>

<table class="table">
    <tr>
        <th>
            Fecha de venta
        </th>
        <th>
            Numero de factura
        </th>
        <th>
            Numero de presupuesto
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    foreach (var p in ViewBag.presupuesto)
    {
        if (item.IdPresupuesto == p.idPresupuesto)
        {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FechaVenta)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FacturaNro)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.presupuesto.PresupuestoNro)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id = item.idVenta }) |
        </td>
    </tr>
        }
    }
}

</table>
